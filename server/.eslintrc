{
  "env": {
    "browser": true
  },
  "extends": "airbnb",
  "rules": {
    "no-underscore-dangle": 0,
    "no-nested-ternary": 1,
    "camelcase": 1,
    "no-lonely-if": 0,
    "no-shadow": 1,
    "max-len": 1,
    "func-names": 0,
    "no-restricted-globals": 1,
    "react/jsx-no-bind": 1,
    "react/prop-types": 1,
    "react/no-did-mount-set-state": 1,
    "jsx-a11y/anchor-is-valid": 1,
    "jsx-a11y/click-events-have-key-events": 1,
    "eol-last": 0,
    "comma-dangle": 0,
    "object-curly-newline": 1
  },

  "parser": "babel-eslint"
}